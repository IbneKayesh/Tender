                                    @*<img src="~/App_Asset/img/lockIconPng.png" class="lock" alt="Lock Profile" style="height:50px;width:50px;" />
                                    <br /><br />*@
                                }
                            </div>
                            <h3 class="profile-username text-center">@Model.ORGANIZATION_NAME</h3>
