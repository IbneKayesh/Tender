                            <div class="text-center">
                                @if (Model.PROFILE_IMAGE != null)
                                {
                                    <img src="~/@Model.PROFILE_IMAGE" class="profile profile-user-img img-fluid img-circle" alt="User profile picture" />
                                }
                                else
                                { 
                                    <img src="~/App_Asset/img/yourcompanylogo.jpg" class="profile-user-img img-fluid img-circle" alt="User profile picture" style=" border: 0px;" />
                                }
                                @if (Model.LOCK_UNLOCK == 1)
