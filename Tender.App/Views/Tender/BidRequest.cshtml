@model Tender.Models.Models.RFQ_TENDER

@{
    ViewBag.Title = "Create Tender";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-12 mt-5">
    <div class="card card-primary card-outline">
        <div class="card-header">
            Create Tender
        </div>

        <div class="card-body">
            @using (Html.BeginForm("ChangePassword", "Accounts", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
                <div class="form-row">
                    <div class="col-md-3 col-sm-6">
                        <div class="form-group">
                            <label class="control-label"></label>
                            <input type="text" id="WORK_MASTER_ID" class="form-control form-control-sm" readonly placeholder="Order Code" />
                            <span asp-validation-for="WORK_MASTER_ID" class="text-danger"></span>

                            @Html.LabelFor(Model => Model.CONTACT_NAME, new { @class = "control-label" })
                            @Html.EditorFor(model => model.CONTACT_NAME, new { htmlAttributes = new { @class = "form-control form-control-sm", placeholder = "Contact Person" } })

                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</div>